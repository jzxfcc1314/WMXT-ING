<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.sxdx.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.sxdx.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FoodInfoServlet</servlet-name>
    <servlet-class>com.sxdx.servlet.FoodInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCarServlet</servlet-name>
    <servlet-class>com.sxdx.servlet.AddCarServlet</servlet-class>
  </servlet>


	<servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/registerservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FoodInfoServlet</servlet-name>
    <url-pattern>/foodinfoservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCarServlet</servlet-name>
    <url-pattern>/addcarservlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
